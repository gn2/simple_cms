-page_title("Create page with #{@layout.name} template")
=render :partial => 'sidebar'

.form_holder
  -form_for( current_object, :url => admin_pages_path(:layout => @layout.id) ) do |f|
    =render :partial => "form", :locals => { :f => f }
    %p.submit
      =f.submit "Create page"
      %span.alternative_button=link_to "Cancel", admin_pages_path