=render :partial => 'sidebar'
-page_title("Creating asset for #{@layout_part.name.humanize}")
/ %h2 Creating asset for #{@layout_part.name.humanize}

.form_holder
  -form_for( @asset, :url => admin_page_assets_path, :html => { :multipart => true } ) do |f|
    =render :partial => "form", :locals => { :f => f }
    %p.submit
      =f.submit "Create asset"
      %span.alternative_button=link_to "Cancel", admin_page_assets_path(@page)